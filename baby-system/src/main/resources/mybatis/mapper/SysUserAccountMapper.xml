<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dupake.system.mapper.SysUserAccountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dupake.system.entity.SysUserAccount">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="account_code" property="accountCode" />
    </resultMap>

    <insert id="insertBatch" useGeneratedKeys="true"
            keyProperty="id">
        INSERT INTO sys_user_account
        (user_id, account_code)
        VALUES
        <foreach item="item" collection="list" separator=",">
            (#{item.userId}, #{item.accountCode})
        </foreach >
    </insert>


</mapper>
